<%
            var hashCode = function (str) {
                var length = str.length;
                var result = 0;
                if (!str || str.length === 0) { return result; }
                
                for (var i = 0; i < str.length; i++) {
                    result = result + str.charCodeAt(i) * Math.pow(31, length-1-i);
                }
                return result;
            };
%>
                <div class="span9 well">
                    <div id="interviewerList">
                        <ul class="thumbnails">
                        <% interviews.Items.forEach(function(interview) {%>
                            <li class="span5 clearfix">
                                <div class="thumbnail clearfix">
                                    <div class="caption" class="pull-left">
                                        <a href="/interview/<%= interview.interviewId.S %>" target="blank">Interview Editor</a>
                                        <h4>
                                        <div>Interview with </div>
                                        <% if (user.candidateType.S === 'interviewer') { %>
                                            <div><%= interview.intervieweeId.S %></div>
                                        <% } else { %>
                                            <div><%= interview.interviewerId.S %></div>
                                        <% } %>
                                        </h4>  
                                   </div>
                                   <div class="rw-ui-container rw-urid-<%= hashCode(interview.interviewId.S) %>"></div>
                                </div>
                            </li>
                          <% });%>
                        </ul>
                </div>
            </div>

