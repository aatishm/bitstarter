<!-- TODO: Formatting and refactor the code-->
                <div class="span9 well">
                    <div id="interviewerList">
                        <ul class="thumbnails">
                        <% interviews.Items.forEach(function(interview) {%>
                            <li class="span5 clearfix">
                                <div class="thumbnail clearfix">
                                    <div class="caption" class="pull-left">
                                        <a href="/interview/<%= interview.interviewId.S %>">Interview Editor</a>
                                        <h4>
                                        <div>Interview with </div>
                                        <% if (user.candidateType.S === 'interviewer') { %>
                                            <%= interview.intervieweeId.S %>
                                        <% } else { %>
                                            <%= interview.interviewerId.S %>
                                        <% } %>
                                        </h4>
                                   </div>
                                </div>
                            </li>
                          <% });%>
                        </ul>
                </div>
            </div>

